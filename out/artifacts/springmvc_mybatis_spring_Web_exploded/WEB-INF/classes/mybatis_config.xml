<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE configuration  
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"  
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<properties>
		<property name="dialect" value="mysql" />
	</properties>
	<!-- <properties resource="jdbc.properties"></properties> -->
	<typeAliases>
		<typeAlias alias="Admin" type="tk.gbl.bean.Admin" />
		<typeAlias alias="MoneyInfo" type="tk.gbl.bean.MoneyInfo" />
		<typeAlias alias="Project" type="tk.gbl.bean.Project" />
		<typeAlias alias="Teacher" type="tk.gbl.bean.Teacher" />
		<typeAlias alias="Unit" type="tk.gbl.bean.Unit" />
	</typeAliases>

	<mappers>
		<mapper resource="tk/gbl/mapper/UnitMapper.xml" />
		<mapper resource="tk/gbl/mapper/AdminMapper.xml" />
		<mapper resource="tk/gbl/mapper/TeacherMapper.xml" />
		<mapper resource="tk/gbl/mapper/ProjectMapper.xml" />
		<mapper resource="tk/gbl/mapper/MoneyInfoMapper.xml" />
	</mappers>
</configuration>  